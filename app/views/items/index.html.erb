<h1>Equipment</h1>
<div class="col-lg-8">
<table class="table">
      <thead class="thead-default">
        <tr>
            <th>Item</th>
            <th>Manufacturer</th>
            <th>Year</th>
        </tr>
    </thead>
    <tbody>
       <% @items.each do |item| %>
        <tr>
            
            <td><%=link_to item.name, item %></td>
            <td><%=item.manufacturer%></td>
            <td><%=item.year%></td>
            
        </tr>
        <% end %>
    </tbody>
</table>
</div>
<div class="col-lg-4">
<%= render "layouts/stats" %>
</div>