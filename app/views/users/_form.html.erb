<%= form_for user do |f|%>
<%= f.label :name %><br>
<%= f.text_field :name %><br>
<%= f.label :email %><br>
<%= f.text_field :email %><br>
<%= f.label :password %><br>
<%= f.password_field :password %><br>
<%= f.label :title %><br>
<%= f.text_field :title %><br>
<%= f.label :location %><br>
<%= f.text_field :location %><br>
<%= f.submit %><br>
<% end %>







